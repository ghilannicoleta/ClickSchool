✅// Transformați din tablou de numere in string cu ajutorul reduce
//Ex: din tabloul [1, 3, 5, 6] => “1356”

let arr = [1, 3, 5, 6];

let str = arr.reduce((prevValue, curentValue) => {
    return prevValue.toString() + curentValue.toString()
})

console.log(str)






✅// Avand lista de cumparaturi aflați cât va costa totul
// rezultatul trebuie sa fie = 14

const shopingList = [ 
 { title: "Milk", price: 1 }, 
 { title: "Bread", price: 2 },
 { title: "Turkey breast", price: 5 }, 
 { title: "Donuts", price: 5 }, 
 { title: "Avocado", price: 1 } 
];

let totalCost = shopingList.reduce((prev, curr) => {
    return prev + curr.price
}, 0)

console.log(totalCost)